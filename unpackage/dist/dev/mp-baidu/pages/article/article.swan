<view class="article"><view class="nav-bar"><scroll-view class="uni-swiper-tab" id="tab-bar" scroll-x="{{true}}" scroll-left="{{scrollLeft}}" show-scrollbar="false"><block s-for="{{navList}}" s-for-item="tab" s-for-index="index" s-key="index"><view class="{{['swiper-tab-list',tabIndex==index?'active':'']}}" id="{{'id'+tab.classid}}" data-current="{{index}}" data-event-opts="{{[['tap',[['tapTab',['$0',index],[[['navList','',index]]]]]]]}}" bindtap="__e">{{tab.name}}</view></block><view class="ts-line" style="{{'left:'+(linePosition)+';'}}"></view><scroll-top vue-id="9fa3d43a-1" isShow="{{newsList[tabIndex].scrollTopShow}}" data-event-opts="{{[['^tap',[['returnTop']]]]}}" bind:tap="__e"></scroll-top></scroll-view></view><swiper class="swiper-box" current="{{tabIndex}}" duration="300" data-event-opts="{{[['change',[['changeTab',['$event']]]]]}}" bindchange="__e"><block s-for="{{newsList}}" s-for-item="tabItem" s-for-index="tabIndex" s-key="tabIndex"><swiper-item class="swiper-item"><scroll-view class="list" scroll-y="{{true}}" scroll-with-animation="{{true}}" nable-back-to-top="{{true}}" refresher-enabled="true" refresher-triggered="{{true}}" scroll-top="{{tabItem.scrollTop}}" data-event-opts="{{[['scrolltolower',[['loadMore',['$event']]]],['scroll',[['scroll',['$event']]]]]}}" bindscrolltolower="__e" bindscroll="__e"><news-list vue-id="{{'9fa3d43a-2-'+tabIndex}}" newsList="{{tabItem.data}}"></news-list><view class="uni-tab-bar-loading"><uni-load-more vue-id="{{'9fa3d43a-3-'+tabIndex}}" loadingType="{{loadingText}}" status="{{tabItem.loadStatus}}"></uni-load-more></view></scroll-view></swiper-item></block></swiper></view>