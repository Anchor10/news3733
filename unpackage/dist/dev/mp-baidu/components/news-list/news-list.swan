<block s-if="{{newsList}}"><view class="news-list"><block s-for="{{newsList}}" s-for-item="item" s-for-index="index" s-key="index"><block><view data-event-opts="{{[['tap',[['goDetail',['$0'],[[['newsList','',index]]]]]]]}}" class="news-item" bindtap="__e"><view class="news-title">{{''+item.title+''}}</view><view class="news-pic"><block s-if="{{item.titlepic!='http://pic3.3733.com/'}}"><image class="pic" src="{{item.titlepic}}" mode="aspectFill"></image></block></view><view class="news-desc">{{''+item.smalltext+''}}</view><view class="news-date"><view>{{item.newstime}}</view><view class="view-num">{{item.onclick}}</view></view></view></block></block></view></block>